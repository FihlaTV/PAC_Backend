<div class="message-cell {{isMine}} sticker" id="{{_id}}" userid="{{userID}}" ts="{{created}}" localId="{{localID}}">
    
    <div class="status status{{status}}">
        
        <span class="sending glyphicon glyphicon-arrow-right"></span>
        <span class="sent glyphicon glyphicon-ok-circle  "></span>
        
    </div>
        
	<div class="avatar">
		<img class="img-circle" src="/api/v2/avatar/user/{{avatarFileId}}" />
    </div>

    <div class="info">
        
        <span class="name"> {{name}} </span>
        <span class="time"> {{formatTime created}} </span>        
        
    </div>
    
    <div class="message">
        <img src="{{{ downloadURL }}}" />
    </div>
    
    <div class="clearfix"></div>
    
</div>

