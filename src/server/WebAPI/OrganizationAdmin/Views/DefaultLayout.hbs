
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Responsive admin dashboard and web application ui kit. ">
    <meta name="keywords" content="blank, starter">

    <title>{{l10n "Spika For Business Admin"}}</title>

    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,300i" rel="stylesheet">

    <!-- Styles -->
    <link href="{{AssetURL}}/css/core.min.css" rel="stylesheet">
    <link href="{{AssetURL}}/css/app.min.css" rel="stylesheet">
    <link href="{{AssetURL}}/css/style.min.css" rel="stylesheet">

    <!-- Favicons -->
    <link rel="apple-touch-icon" href="{{AssetURL}}/img/apple-touch-icon.png">
    <link rel="icon" href="{{AssetURL}}/img/favicon.png">
  </head>

  <body>

    <!-- Preloader -->
    <div class="preloader">
      <div class="spinner-dots">
        <span class="dot1"></span>
        <span class="dot2"></span>
        <span class="dot3"></span>
      </div>
    </div>

    <!-- Sidebar -->
    <aside class="sidebar sidebar-icons-right sidebar-icons-boxed sidebar-expand-lg">
      <header class="sidebar-header">
        <a class="logo-icon" href="../index.html"><img src="{{AssetURL}}/img/logo-icon-light.png" alt="logo icon"></a>
        <span class="logo">
          <a href="../index.html"><img src="{{AssetURL}}/img/logo-light.png" alt="logo"></a>
        </span>
        <span class="sidebar-toggle-fold"></span>
      </header>

      <nav class="sidebar-navigation">
        <ul class="menu">

          <li class="menu-item {{{printIfEqual page 'home' 'active'}}}">
            <a class="menu-link" href="/admin/home">
              <span class="icon fa fa-tachometer"></span>
              <span class="title">{{l10n "Dashboard"}}</span>
            </a>
          </li>
          
          <li class="menu-category">{{l10n "Credentials & Permission"}}</li>

          <li class="menu-item {{{printIfEqual page 'user-list' 'active'}}}">
            <a class="menu-link" href="/admin/user/list">
              <span class="icon fa fa-user"></span>
              <span class="title">{{l10n "Users"}}</span>
            </a>
          </li>

          <li class="menu-item {{{printIfEqual page 'group-list' 'active'}}}">
            <a class="menu-link" href="/admin/group/list">
              <span class="icon fa fa-users"></span>
              <span class="title">{{l10n "Groups"}}</span>
            </a>
          </li>

          <li class="menu-item {{{printIfEqual page 'department-list' 'active'}}}">
            <a class="menu-link" href="/admin/department/list">
              <span class="icon fa fa-sitemap"></span>
              <span class="title">{{l10n "Departments"}}</span>
            </a>
          </li>

          <li class="menu-category">{{l10n "Integrations"}}</li>

          <li class="menu-item {{{printIfEqual page 'api-list' 'active'}}}">
            <a class="menu-link" href="/admin/apikey/list">
              <span class="icon fa fa-key"></span>
              <span class="title">{{l10n "API Keys"}}</span>
            </a>
          </li>

          <li class="menu-item {{{printIfEqual page 'webhook-list' 'active'}}}">
            <a class="menu-link" href="/admin/webhook/list">
              <span class="icon fa fa-exchange"></span>
              <span class="title">{{l10n "Webhooks"}}</span>
            </a>
          </li>

          <li class="menu-category">{{l10n "Settings"}}</li>

          <li class="menu-item {{{printIfEqual page 'sticker-list' 'active'}}}">
            <a class="menu-link" href="/admin/sticker/list">
              <span class="icon fa fa-smile-o"></span>
              <span class="title">{{l10n "Stickers"}}</span>
            </a>
          </li>

          <li class="menu-item {{{printIfEqual page 'settings' 'active'}}}">
            <a class="menu-link" href="/admin/settings">
              <span class="icon fa fa-cog"></span>
              <span class="title">{{l10n "Settings"}}</span>
            </a>
          </li>


        </ul>
      </nav>

    </aside>
    <!-- END Sidebar -->


    <!-- Topbar -->
    <header class="topbar">
      <div class="topbar-left">
        <span class="topbar-btn sidebar-toggler"><i>&#9776;</i></span>
      </div>

      <div class="topbar-right">

        <ul class="topbar-btns">
          <li class="dropdown">
            <span class="topbar-btn" data-toggle="dropdown" aria-expanded="true">
              <img class="avatar" src="/api/v2/avatar/user/{{avatarFileId}}" alt="..." />
            </span>
            <div class="dropdown-menu dropdown-menu-right" x-placement="bottom-end" style="position: absolute; top: 65px; left: -120px; will-change: top, left;">
              <span class="dropdown-item"> 
                
                {{l10n "User:"}} <strong>{{signInUserName}}</strong><br />
                {{l10n "Organization:"}} <strong>{{signInOrganization}}</strong>

              </span>
              <div class="dropdown-divider"></div>
              <a class="dropdown-item" href="/admin/signout">
                <i class="ti-power-off"></i>{{l10n "Logout"}}
              </a>
            </div>
          </li>

        </ul>

      </div>

    </header>
    <!-- END Topbar -->


    <!-- Main container -->
    <main class="main-container">
      <div class="main-content">

        {{{body}}}

      </div><!--/.main-content -->

      <!-- Footer -->
      <footer class="site-footer">
        <div class="row">
          <div class="col-md-6">
            <p class="text-center text-md-left">Copyright Â© 2018 <a href="http://clover.studio">Clover Studio</a>. All rights reserved.</p>
          </div>

          <div class="col-md-6">
            <ul class="nav nav-primary nav-dotted nav-dot-separated justify-content-center justify-content-md-end">

              <li class="nav-item">
                <a class="nav-link" href="https://itunes.apple.com/jp/app/spika-for-business/id1143027894?mt=8" target="_blank">{{l10n "iOS App"}}</a>
              </li>
              
              <li class="nav-item">
                <a class="nav-link" href="https://play.google.com/store/apps/details?id=com.clover_studio.spikaenterprisev2&hl=en" target="_blank">{{l10n "Android App"}}</a>
              </li>
              
              <li class="nav-item">
                <a class="nav-link" href="/" target="_blank">{{l10n "WebClient"}}</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="../help/articles.html">Documentation</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="../help/faq.html">Help</a>
              </li>
            </ul>
          </div>
        </div>
      </footer>
      <!-- END Footer -->

    </main>
    <!-- END Main container -->



    <!-- Global quickview -->
    <div id="qv-global" class="quickview" data-url="{{AssetURL}}/data/quickview-global.html">
      <div class="spinner-linear">
        <div class="line"></div>
      </div>
    </div>
    <!-- END Global quickview -->



    <!-- Scripts -->
    <script src="{{AssetURL}}/js/core.min.js"></script>
    <script src="{{AssetURL}}/js/app.min.js"></script>
    <script src="{{AssetURL}}/js/script.min.js"></script>

  </body>
</html>
